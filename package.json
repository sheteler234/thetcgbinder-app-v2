{
  "name": "thetcgbinder-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Premium trading card game e-commerce platform with interactive binder experience",
  "author": "Daniel Shalar <danielshalar5@gmail.com>",
  "homepage": "https://thetcgbinder.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/thetcgbinder-app.git"
  },
  "keywords": [
    "trading-cards",
    "tcg",
    "yu-gi-oh",
    "pokemon",
    "e-commerce",
    "react",
    "typescript",
    "vite"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:email-server\" --names \"Frontend,EmailServer\" --prefix-colors \"cyan,green\"",
    "dev:frontend": "vite",
    "dev:email-server": "cd server && npm run dev",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "email-server": "cd server && npm start",
    "setup-email-server": "cd server && npm install"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@paypal/react-paypal-js": "^8.8.3",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
